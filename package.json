{
  "name": "@kubevirt-ui/kubevirt-api",
  "version": "1.5.0",
  "description": "kubevirt OpenAPI automation for TypeScript",
  "type": "module",
  "exports": {
    "./console": {
      "import": {
        "types": "./dist/console/index.d.ts",
        "import": "./dist/console/index.js"
      },
      "require": {
        "types": "./dist/console/index.d.cts",
        "require": "./dist/console/index.cjs"
      }
    },
    "./containerized-data-importer": {
      "import": {
        "types": "./dist/containerized-data-importer/index.d.ts",
        "import": "./dist/containerized-data-importer/index.js"
      },
      "require": {
        "types": "./dist/containerized-data-importer/index.d.cts",
        "require": "./dist/containerized-data-importer/index.cjs"
      }
    },
    "./kubevirt": {
      "import": {
        "types": "./dist/kubevirt/index.d.ts",
        "import": "./dist/kubevirt/index.js"
      },
      "require": {
        "types": "./dist/kubevirt/index.d.cts",
        "require": "./dist/kubevirt/index.cjs"
      }
    },
    "./kubernetes": {
      "import": {
        "types": "./dist/kubernetes/index.d.ts",
        "import": "./dist/kubernetes/index.js"
      },
      "require": {
        "types": "./dist/kubernetes/index.d.cts",
        "require": "./dist/kubernetes/index.cjs"
      }
    },
    "./nmstate": {
      "import": {
        "types": "./dist/nmstate/index.d.ts",
        "import": "./dist/nmstate/index.js"
      },
      "require": {
        "types": "./dist/nmstate/index.d.cts",
        "require": "./dist/nmstate/index.cjs"
      }
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "generate": "bash ./scripts/generate.sh",
    "generate:nmstate": "bash ./scripts/generate-nmstate.sh",
    "generate:fix-console": "bash ./scripts/fix-console-models.sh",
    "generate:fix": "bash ./scripts/fix-generate-errors.sh",
    "generate:docs": "typedoc --out docs/  --gitRevision main --gitRemote upstream --readme none --entryPoints \"kubevirt/models/index.ts\" \"kubernetes/models/index.ts\" \"nmstate/models/index.ts\" \"containerized-data-importer/models/index.ts\" \"console/index.ts\"  --plugin typedoc-plugin-reference-excluder --excludedFunctionOrMethod \".*\" ",
    "clean": "rimraf ./generated",
    "clean:all": "rimraf ./docs ./generated ./kubevirt ./containerized-data-importer ./console/core/index.ts ./console-core-models.ts ./kubernetes ./nmstate/models ./nmstate/index.ts ./nmstate/runtime.ts swagger-*.json",
    "lint": "eslint console kubevirt kubernetes containerized-data-importer nmstate --color --no-inline-config",
    "lint:fix": "yarn lint --fix",
    "build": "tsup",
    "test": "echo \"It's all OK\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubevirt-ui/kubevirt-api.git"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@openshift-console/dynamic-plugin-sdk": "^0.0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "crdtoapi": "^0.0.19",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.5.1",
    "rimraf":"6.1.2",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.10",
    "tsup": "^8.5.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-reference-excluder": "^1.0.0",
    "typescript": "^4.5.5"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "console/index.ts",
      "containerized-data-importer/index.ts", 
      "kubevirt/index.ts",
      "kubernetes/index.ts",
      "nmstate/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "minify": true,
    "dts": true
  },
  "keywords": [
    "kubevirt",
    "kubernetes",
    "containerized-data-importer",
    "openapi",
    "typescript",
    "openshift",
    "openshift-console"
  ],
  "author": "Yaacov Zamir <yzamir@redhat.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kubevirt-ui/kubevirt-api/issues"
  },
  "homepage": "https://github.com/kubevirt-ui/kubevirt-api#readme"
}